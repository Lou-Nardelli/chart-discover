{
  "name": "teamwear",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "prepare": "node .husky/install.mjs",
    "clean": "rm -rf .next node_modules",
    "dev": "dotenvx run --strict -f .env.development -fk .env.keys --overload -- next dev --turbopack",
    "build": "dotenvx run --strict -f .env.production -fk .env.keys --overload -- next build",
    "start": "dotenvx run --strict -f .env.production -fk .env.keys --overload -- next start",
    "test": "echo 'No tests yet'",
    "lint-staged": "lint-staged",
    "lint": "biome check --fix .",
    "lint:staged": "biome check --fix --staged --error-on-warnings --no-errors-on-unmatched",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "next": "^15.3.4",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.0.5",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@dotenvx/dotenvx": "^1.45.1",
    "@tailwindcss/postcss": "^4.1.11",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.8.3",
    "ultracite": "^5.0.19"
  },
  "engines": {
    "pnpm": ">=9",
    "node": ">=22 <=22.14.0"
  }
}
